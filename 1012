#include <stdio.h>

int main()
{
    float A, B, C;
    double pi, TRIANGULO, CIRCULO, TRAPEZIO, QUADRADO, RETANGULO;
    pi = 3.14159;
    scanf("%f %f %f", &A, &B, &C);
    TRIANGULO = A * C * 0.5;
    CIRCULO = pi * (C * C);
    TRAPEZIO = (A + B) /2 * C;
    QUADRADO = B * B;
    RETANGULO = A * B;
    printf("TRIANGULO: %.3lf\nCIRCULO: %.3lf\nTRAPEZIO: %.3lf\nQUADRADO: %.3lf\nRETANGULO: %.3lf\n", TRIANGULO, CIRCULO, TRAPEZIO, QUADRADO, RETANGULO);
    return 0;
}
